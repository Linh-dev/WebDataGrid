<!--<PresetInfo PresetName="BasicClock" PresetDescription="Custom Preset Created 5/31/2007 5:57:08 PM" PresetType="All" />-->
<igGauge:UltraGauge BackColor="Transparent" ForeColor="ControlLightLight" Width="250" Height="250">
  <Gauges>
    <igGaugeProp:RadialGauge>
      <Scales>
        <igGaugeProp:RadialGaugeScale EndAngle="630" StartAngle="270" Key="hour">
          <MinorTickmarks EndWidth="1" EndExtent="80" Frequency="0.2" StartExtent="77">
            <BrushElements>
              <igGaugeProp:SolidFillBrushElement Color="225, 225, 225" />
            </BrushElements>
            <StrokeElement>
              <BrushElements>
                <igGaugeProp:SolidFillBrushElement Color="240, 240, 240" />
              </BrushElements>
            </StrokeElement>
          </MinorTickmarks>
          <Markers>
            <igGaugeProp:RadialGaugeNeedle MidWidth="2" Value="1" EndWidth="2" MidExtent="0" AllowDrag="True" EndExtent="40" StartWidth="2" WidthMeasure="Percent" Key="hour">
              <BrushElements>
                <igGaugeProp:SolidFillBrushElement Color="255, 61, 22" />
              </BrushElements>
              <Response />
            </igGaugeProp:RadialGaugeNeedle>
          </Markers>
          <Axes>
            <igGaugeProp:NumericAxis EndValue="12" />
          </Axes>
          <MajorTickmarks EndWidth="3" EndExtent="80" StartExtent="72" StartWidth="3">
            <BrushElements>
              <igGaugeProp:SolidFillBrushElement Color="189, 189, 189" />
            </BrushElements>
          </MajorTickmarks>
          <Labels Orientation="Horizontal" PostInitial="1" SpanMaximum="18" Extent="60" Font="Trebuchet MS, 16px, style=Bold">
            <BrushElements>
              <igGaugeProp:SolidFillBrushElement Color="189, 189, 189" />
            </BrushElements>
          </Labels>
        </igGaugeProp:RadialGaugeScale>
        <igGaugeProp:RadialGaugeScale EndAngle="630" StartAngle="270" Key="minute">
          <Markers>
            <igGaugeProp:RadialGaugeNeedle MidWidth="2" Value="10" EndWidth="2" MidExtent="0" AllowDrag="True" EndExtent="60" StartWidth="2" WidthMeasure="Percent" Key="minute">
              <BrushElements>
                <igGaugeProp:SolidFillBrushElement Color="255, 61, 22" />
              </BrushElements>
              <Response />
            </igGaugeProp:RadialGaugeNeedle>
          </Markers>
          <Axes>
            <igGaugeProp:NumericAxis EndValue="60" />
          </Axes>
        </igGaugeProp:RadialGaugeScale>
        <igGaugeProp:RadialGaugeScale EndAngle="630" StartAngle="270" Key="second">
          <Markers>
            <igGaugeProp:RadialGaugeNeedle MidWidth="1" Value="20" EndWidth="1" MidExtent="0" AllowDrag="True" EndExtent="70" StartExtent="-15" StartWidth="1" WidthMeasure="Percent" Key="second">
              <Anchor RadiusMeasure="Percent" Radius="6">
                <BrushElements>
                  <igGaugeProp:SimpleGradientBrushElement EndColor="64, 64, 64" StartColor="Gainsboro" GradientStyle="BackwardDiagonal" />
                </BrushElements>
                <StrokeElement Thickness="2">
                  <BrushElements>
                    <igGaugeProp:RadialGradientBrushElement FocusScalesString="0, 0" CenterPointString="75, 25" SurroundColor="Gray" CenterColor="WhiteSmoke" />
                  </BrushElements>
                </StrokeElement>
              </Anchor>
              <BrushElements>
                <igGaugeProp:SolidFillBrushElement Color="255, 61, 22" />
              </BrushElements>
              <Response />
              <StrokeElement Thickness="0" />
            </igGaugeProp:RadialGaugeNeedle>
          </Markers>
          <Axes>
            <igGaugeProp:NumericAxis EndValue="60" />
          </Axes>
        </igGaugeProp:RadialGaugeScale>
      </Scales>
      <Dial>
        <BrushElements>
          <igGaugeProp:BrushElementGroup>
            <BrushElements>
              <igGaugeProp:MultiStopRadialGradientBrushElement FocusScalesString="0.8, 0.8" CenterPointString="50, 50">
                <ColorStops>
                  <igGaugeProp:ColorStop Color="161, 161, 161" />
                  <igGaugeProp:ColorStop Color="96, 96, 96" Stop="0.4758621" />
                  <igGaugeProp:ColorStop Color="89, 89, 89" Stop="1" />
                </ColorStops>
              </igGaugeProp:MultiStopRadialGradientBrushElement>
              <igGaugeProp:SolidFillBrushElement Color="Gray" RelativeBoundsMeasure="Percent" RelativeBounds="4, 4, 93, 93" />
              <igGaugeProp:MultiStopRadialGradientBrushElement FocusScalesString="0, 0" CenterPointString="50, 50" RelativeBoundsMeasure="Percent" RelativeBounds="4, 4, 93, 93">
                <ColorStops>
                  <igGaugeProp:ColorStop Color="Transparent" />
                  <igGaugeProp:ColorStop Color="141, 141, 141" Stop="0.06206897" />
                  <igGaugeProp:ColorStop Color="82, 82, 82" Stop="0.07586207" />
                  <igGaugeProp:ColorStop Color="45, 45, 45" Stop="0.1448276" />
                  <igGaugeProp:ColorStop Color="30, 30, 30" Stop="0.2448276" />
                  <igGaugeProp:ColorStop Color="60, 60, 60" Stop="1" />
                </ColorStops>
              </igGaugeProp:MultiStopRadialGradientBrushElement>
            </BrushElements>
          </igGaugeProp:BrushElementGroup>
        </BrushElements>
        <StrokeElement>
          <BrushElements>
            <igGaugeProp:SolidFillBrushElement Color="Silver" />
          </BrushElements>
        </StrokeElement>
      </Dial>
    </igGaugeProp:RadialGauge>
  </Gauges>
</igGauge:UltraGauge>